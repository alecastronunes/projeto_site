<!DOCTYPE html>
<html lang="en">
    <%- include('shared/head') %>

<body>
    <%-include('shared/header')%>

    <section class="votacao">
        <h2>Vote no seu emoji favorito.</h2>
        
        
        <div class="container">
            <% for (let votacao of votacoes){ %>
                <div> 
                    <img src="<%= votacao.img%>" alt= "">
                    <h3><%= votacao.nome %></h3>
                </div>
           <% }%>

        </div>

        <form action="">
            <input type="text" placeholder="Qua seu emoji favorito" name="emoji">
            <textarea name="porque" id="" cols="30" rows="10"></textarea>
            <button>:)</button>
        </form>
    </section>

</body>
</html>